{
  "name": "blank-node-worker",
  "version": "0.0.2",
  "description": "JavaScript task runner for Blank platform",
  "main": "lib/worker.js",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "handlebars": "^4.0.5",
    "jszip": "^3.0.0",
    "minimist": "latest",
    "moment": "^2.13.0",
    "mongodb": "^2.1.18",
    "nodemailer": "^2.4.2",
    "scrypt": "^6.0.3",
    "sift": "^3.2.1",
    "winston": "^2.2.0",
    "ws": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "mocha": "latest"
  },
  "scripts": {
    "startHelper": "SR=\"ws://localhost:1234\" npm start",
    "prestart": "rm -rf ./lib && ./node_modules/babel-cli/bin/babel.js ./blank-js-core -d lib/blank-js-core && ./node_modules/babel-cli/bin/babel.js src -d lib",
    "start": "NODE_PATH='./local:./lib/blank-js-core' node ./lib/worker.js $SR",
    "pretest": "rm -rf ./lib && ./node_modules/babel-cli/bin/babel.js ./blank-js-core -d lib/blank-js-core && ./node_modules/babel-cli/bin/babel.js src -d lib",
    "test": "NODE_PATH='./local:./lib/blank-js-core' NODE_ENV='test' mocha -t 5000 ./tests"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "keywords": [
    "Blank"
  ],
  "author": {
    "email": "info@getblank.net",
    "name": "Evgeniy Kuvshinov & Evgeniy Ivakha"
  },
  "license": "GPL-3.0"
}
