{
  "name": "blank-node-worker",
  "version": "0.0.1",
  "description": "JavaScript task runner for Blank platform",
  "main": "src/worker.js",
  "dependencies": {
    "minimist": "latest",
    "mongodb": "^2.1.18",
    "scrypt": "^6.0.3",
    "ws": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "mocha": "latest"
  },
  "scripts": {
    "build": "rm -rf ./lib && ./node_modules/babel-cli/bin/babel.js src -d lib",
    "startHelper": "SR=\"ws://localhost:1234\" npm start",
    "start": "babel src -d lib && NODE_PATH='../../common' node ./lib/worker.js $SR",
    "test": "babel src -d lib && NODE_PATH='../../common' mocha ./tests"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "keywords": [
    "Blank"
  ],
  "author": "Blank",
  "license": "MIT"
}