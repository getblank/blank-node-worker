{
  "name": "blank-node-worker",
  "version": "0.0.1",
  "description": "JavaScript task runner for Blank platform",
  "main": "src/worker.js",
  "dependencies": {
    "minimist": "latest",
    "mongodb": "^2.1.18",
    "scrypt": "^6.0.3",
    "winston": "^2.2.0",
    "ws": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "mocha": "latest"
  },
  "scripts": {
    "startHelper": "SR=\"ws://localhost:1234\" npm start",
    "prestart": "rm -rf ./lib && ./node_modules/babel-cli/bin/babel.js src -d lib",
    "start": "NODE_PATH='./blank-js-core' node ./lib/worker.js $SR",
    "pretest": "rm -rf ./lib && ./node_modules/babel-cli/bin/babel.js src -d lib",
    "test": "NODE_PATH='./blank-js-core' TESTING='TESTING' mocha ./tests"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "keywords": [
    "Blank"
  ],
  "author": {
    "email": "info@getblank.net",
    "name": "Evgeniy Kuvshinov & Evgeniy Ivakha"
  },
  "license": "GPL-3.0"
}
